{
  "name": "youdeploy",
  "version": "0.0.1",
  "description": "An open source project to deploy your frontend in one click on your own resources.",
  "main": "build/index.js",
  "author": "Alex",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "deploy": "npx ts-node ./src/index.ts",
    "update-npm": "yarn run build && cp package.json build && npm version patch && npm publish",
    "semantic-release": "semantic-release"
  },
  "bin": {
    "youdeploy": "index.js"
  },
  "dependencies": {
    "@types/node": "^18.15.11",
    "aws-sdk": "^2.1347.0",
    "semantic-release": "^21.0.1",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "semantic-release": "^21.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aballerr/easy-deploy.git"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
