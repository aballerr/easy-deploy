    "archiver": "^5.3.1",
    "aws-cdk-lib": "^2.81.0",
    "aws-lambda": "^1.0.7",
    "chalk": "4.1.2",
    "constructs": "^10.2.38",
    "cross-spawn": "^7.0.3",
    "esbuild": "^0.17.19",
    "find-package-json": "^1.2.0",
    "fs-extra": "^11.1.1",
    "mime-types": "^2.1.35",
    "supports-hyperlinks": "^3.0.0",
    "terminal-link": "^3.0.0",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
